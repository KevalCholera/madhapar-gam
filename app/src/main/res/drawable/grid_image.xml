<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--<item android:drawable="@drawable/icon_grid_fill" android:state_checked="true"/>-->
    <!--<item android:drawable="@drawable/ic_icon" android:state_checked="false"/>-->
    <item android:state_checked="true" android:state_pressed="false">
        <bitmap android:src="@drawable/icon_grid_fill" android:gravity="center" />
    </item>
    <item android:state_checked="false" android:state_pressed="false">
        <bitmap android:src="@drawable/ic_icon" android:gravity="center" />
    </item>
    <item android:state_checked="true" android:state_pressed="true">
        <bitmap android:src="@drawable/ic_icon" android:gravity="center" />
    </item>
    <item android:state_checked="false" android:state_pressed="true">
        <bitmap android:src="@drawable/ic_icon" android:gravity="center" />
    </item>
</selector>