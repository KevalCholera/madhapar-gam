<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:dither="true"
            android:shape="oval"
            android:useLevel="true">
            <gradient
                android:startColor="@color/colorOrange"
                android:endColor="@color/colorOrange"
                 />
            <size
                android:width="25dp"
                android:height="25dp" />
            <stroke
                android:width="1dp"
                android:color="@color/colorOrange" />
            <corners
                android:radius="30dp" />
        </shape>
    </item>
</selector>